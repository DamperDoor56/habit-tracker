{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "jsx": "react-native",
    "allowJs": true,
    "lib": ["esnext"],
    "module": "Node16",
    "moduleResolution": "node16",
    "target": "esnext",
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "customConditions": ["react-native"]
  }
}
